{
  "extends": "solium:recommended",
  "plugins": ["security"],
  "rules": {
    "no-experimental": ["off"],
    "quotes": ["error", "double"],
    "indentation": ["error", 4],
    "linebreak-style": ["error", "unix"],
    "security/no-inline-assembly": ["off"],
    "security/no-low-level-calls": ["off"]
  }
}
